#% Do not delete the line below; configure depends on this
#  \DeclareLaTeXClass[tudthesis]{TU Darmstadt Thesis}
#  \DeclareCategory{Reports}

Format 66

Input scrreprt.layout

# Provides amsmath         1
Provides latexsym       1
Provides amssymb       1
Provides makeidx         1

TitleLatexName "makethesistitle"



Style Group
	CopyStyle             Author
	LatexName             group
End


Style Department  
	CopyStyle             Author
	LatexName             department
End

Style Referee  
	CopyStyle             Author
	LatexName             referee
	LatexParam            Mustermann

	Argument 1
		LabelString	"Required Layout Argument 1"
		Mandatory	1
	EndArgument
End


Style Thesistitle
	CopyStyle             Title
	LatexName             thesistitle

	Argument 1
		LabelString	"Required Layout Argument 1"
		Mandatory	1
	EndArgument
End

NoStyle Title
